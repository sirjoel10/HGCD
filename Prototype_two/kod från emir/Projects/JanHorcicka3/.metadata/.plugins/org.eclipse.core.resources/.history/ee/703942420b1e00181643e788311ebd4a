#include "main.h"
#include "stm32l4xx.h"
#include "stm32l4xx_nucleo.h"

#include "stdio.h"
#include "stdlib.h"


int main(void)
{

	SystemInit();
	HAL_Init();
	SystemClock_Config();
	MX_GPIO_Init();

while(1)
{
	HAL_Delay(500);
	HAL_GPIO_TogglePin(LD2_GPIO_Port, LD2_Pin);
}
}
